<span id="cb5-10"><span class="co" style="color: #5E5E5E;">  </span><span class="va" style="color: #111111;">kernel</span><span class="co" style="color: #5E5E5E;"> -- </span><span class="va" style="color: #111111;">runmem</span><span class="ot" style="color: #003B4F;">;</span></span>
<span id="cb5-11"><span class="co" style="color: #5E5E5E;">  </span><span class="va" style="color: #111111;">sleep</span><span class="co" style="color: #5E5E5E;"> -- </span><span class="va" style="color: #111111;">swap</span><span class="ot" style="color: #003B4F;">;</span></span>
<span id="cb5-12"><span class="co" style="color: #5E5E5E;">  </span><span class="va" style="color: #111111;">swap</span><span class="co" style="color: #5E5E5E;"> -- </span><span class="va" style="color: #111111;">runswap</span><span class="ot" style="color: #003B4F;">;</span></span>
